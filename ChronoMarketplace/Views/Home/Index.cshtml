@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4"><FONT COLOR="white">Chrono Marketplace</FONT></h1>
    <p><FONT COLOR="white">Created by Parin Shah</FONT></p>
</div>
<form class="d-flex">
    <input class="form-control me-2" type="search" placeholder="Search for products..." aria-label="Search">
    <button class="btn btn-outline-light" type="submit">Search</button>
</form>



<!-- Carousel -->
<div id="demo" class="carousel slide" data-bs-ride="carousel">

    <!-- Indicators/dots -->
    <div class="carousel-indicators">
        <button type="button" data-bs-target="#demo" data-bs-slide-to="0" class="active"></button>
        <button type="button" data-bs-target="#demo" data-bs-slide-to="1"></button>
        <button type="button" data-bs-target="#demo" data-bs-slide-to="2"></button>
        <button type="button" data-bs-target="#demo" data-bs-slide-to="3"></button>
        <button type="button" data-bs-target="#demo" data-bs-slide-to="4"></button>
        <button type="button" data-bs-target="#demo" data-bs-slide-to="5"></button>
           </div>

    <!-- The slideshow/carousel -->
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="https://cdn.pixabay.com/photo/2020/01/31/08/38/rolex-4807479_1280.jpg" alt="Rolex" class="d-block w-100">
        </div>
        <div class="carousel-item">
            <img src="https://images.unsplash.com/photo-1626624386327-5204714bae9f?q=80&w=1740&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" alt="Patek Philippe" class="d-block w-100">
        </div>
        <div class="carousel-item">
            <img src="https://cdn.pixabay.com/photo/2023/08/07/03/58/role-8174271_1280.jpg" alt="Audemars Piguet" class="d-block w-100">
        </div>
        <div class="carousel-item">
            <img src="https://images.unsplash.com/photo-1696321408191-62d7363743b4?q=80&w=1740&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" alt="Omega" class="d-block w-100">
        </div>
               <div class="carousel-item">
            <img src="https://images.unsplash.com/photo-1612817159576-986a0b7a4165?q=80&w=1625&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" alt="Seiko" class="d-block w-100">
        </div>
        <div class="carousel-item">
            <img src="https://images.unsplash.com/photo-1605583972593-1fda24a7bea2?q=80&w=1744&auto=format&fit=crop&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D" alt="Casio" class="d-block w-100">
        </div>
    </div>

    <!-- Left and right controls/icons -->
    <button class="carousel-control-prev" type="button" data-bs-target="#demo" data-bs-slide="prev">
        <span class="carousel-control-prev-icon"></span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#demo" data-bs-slide="next">
        <span class="carousel-control-next-icon"></span>
    </button>
</div>
